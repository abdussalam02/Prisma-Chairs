{
    "version": 2,
    "installCommand": "pip install",
    "builds": [
        { "src": "*.html", "use": "@vercel/static" },
        { "src": "*.py", "use": "@vercel/python" },
        { "src": "*.js", "use": "@vercel/js" }
    ],
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}